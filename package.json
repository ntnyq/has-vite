{
  "name": "has-vite",
  "type": "module",
  "version": "0.2.1",
  "packageManager": "pnpm@10.15.0",
  "description": "Check if a project is using vite",
  "keywords": [
    "check",
    "has-vite",
    "vite"
  ],
  "license": "MIT",
  "author": {
    "name": "ntnyq",
    "email": "ntnyq13@gmail.com"
  },
  "homepage": "https://github.com/ntnyq/has-vite#readme",
  "repository": "ntnyq/has-vite",
  "bugs": {
    "url": "https://github.com/ntnyq/has-vite/issues"
  },
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    }
  },
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "sideEffects": false,
  "scripts": {
    "build": "tsdown",
    "dev": "tsdown --watch",
    "lint": "eslint",
    "prepare": "husky",
    "prepublishOnly": "pnpm run build",
    "release": "run-s release:check release:version",
    "release:check": "run-s lint typecheck test",
    "release:version": "bumpp",
    "test": "vitest --typecheck",
    "typecheck": "tsc --noEmit"
  },
  "devDependencies": {
    "@ntnyq/eslint-config": "^5.4.1",
    "@ntnyq/prettier-config": "^3.0.1",
    "@types/node": "^24.3.0",
    "bumpp": "^10.2.3",
    "eslint": "^9.33.0",
    "husky": "^9.1.7",
    "nano-staged": "^0.8.0",
    "npm-run-all2": "^8.0.4",
    "prettier": "^3.6.2",
    "tsdown": "^0.14.1",
    "typescript": "^5.9.2",
    "vitest": "^3.2.4"
  },
  "nano-staged": {
    "*.{js,ts,mjs,cjs,md,yml,yaml,json}": "eslint --fix"
  }
}
